                },
                "die_count": {
                    "$id": "#/properties/die_count",
                    "type": "integer",
                    "title": "The Die_count Schema",
                    "default": 0,
                    "examples": [
                        4
                    ]
                },
                "per_modifier": {
                    "$id": "#/properties/per_modifier",
                    "#ref": "#/definitions/modifier"
                },
                "reroll_value": {
                    "$id": "#/properties/reroll_value",
                    "type": "integer",
                    "title": "The reroll_value Schema",
                    "default": 0,
                    "examples": [
                        0,
                        4,
                        10
                    ]
                },
                "post_modifier": {
                    "$id": "#/properties/post_modifier",
                    "#ref": "#/definitions/modifier"
                },
                "roll_modifier": {
                    "$id": "#/properties/roll_modifier",
                    "#ref": "#/definitions/modifier"
                },
                "reroll_condition": {
                    "$id": "#/properties/reroll_condition",
                    "type": "string",
                    "enumb": [
                        "==",
                        "<=",
                        "<",
                        ">==",
                        ">"
                    ],
                    "title": "The reroll_condition Schema",
                    "default": "=="
                }
            },
            "definitions": {
                "modifier": {
                    "type": "object",
                    "required": [
                        "type",
                        "value"
                    ],
                    "properties": {
                        "type": {
                            "enum": [
                                "+",
                                "-",
                                "*",
                                "/"
                            ],
                            "type": "string",
                            "default": "+"
                        },
                        "value": {
                            "type": "integer",
                            "default": 0,
                            "examples": [
                                3,
                                4,
                                10,
                                100
                            ]
                        }
                    }
                }
            }
        },
        "specification": "Draft-07",
        "enabled": true,
        "deprecated": false
    }
